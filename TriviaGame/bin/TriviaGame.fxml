<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TriviaGameController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane fx:id="scenePane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="aButton" focusTraversable="false" layoutX="160.0" layoutY="210.0" mnemonicParsing="false" onAction="#aPressed" text="A">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="bButton" focusTraversable="false" layoutX="395.0" layoutY="210.0" mnemonicParsing="false" onAction="#bPressed" text="B">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="cButton" focusTraversable="false" layoutX="160.0" layoutY="288.0" mnemonicParsing="false" onAction="#cPressed" text="C">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="dButton" focusTraversable="false" layoutX="395.0" layoutY="288.0" mnemonicParsing="false" onAction="#dPressed" text="D">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Text fx:id="textOne" layoutX="87.0" layoutY="161.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Question" wrappingWidth="450.0">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Text fx:id="textTwo" layoutX="175.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Trivia Game Of World Cup">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Button focusTraversable="false" layoutX="573.0" layoutY="14.0" mnemonicParsing="false" onAction="#restartPressed" text="Restart" />
            <Button focusTraversable="false" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#quitPressed" text="Quit" />
            <Button fx:id="nextQuestion" focusTraversable="false" layoutX="14.0" layoutY="91.0" mnemonicParsing="false" onAction="#nextQuestionPressed" text="Next Question" />
         </children>
    </AnchorPane>
  </children>
</VBox>
